  "scripts": {
    "clean": "better-npm-run clean",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "better-npm-run deploy"
  },
  "betterScripts": {
    "clean": {
      "command": "rimraf dist/*",
      "env": {}
    },
    "build:demo": {
      "command": "webpack -d",
      "env": {
        "DEMO": true
      }
    },
    "deploy": {
      "command": "better-npm-run clean && better-npm-run build:demo"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
